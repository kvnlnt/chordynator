<div class="tab" chord="<%= chord %>">
    <svg viewBox="0 0 100 50" version="1.1">

        <!-- Buttons -->
        <!-- <image button="remove" x="77" y="0" width="15" height="15" class="remove" xlink:href="images/delete.svg"/> -->
        <!-- <image button="handle" x="8"  y="0" width="15" height="15" class="move"   xlink:href="images/handle.svg"/> -->
        <g button="handle" class="move" transform="translate(7,0)">
            <rect fill="#000000" width="15" height="15" />
            <path transform="scale(0.20) translate(22,17)" fill="#FFFFFF" d="M4,10h24c1.104,0,2-0.896,2-2s-0.896-2-2-2H4C2.896,6,2,6.896,2,8S2.896,10,4,10z M28,14H4c-1.104,0-2,0.896-2,2  s0.896,2,2,2h24c1.104,0,2-0.896,2-2S29.104,14,28,14z M28,22H4c-1.104,0-2,0.896-2,2s0.896,2,2,2h24c1.104,0,2-0.896,2-2  S29.104,22,28,22z"
            />
        </g>

        <g button="remove" class="remove" transform="translate(77,0)">
            <rect fill="#000000" width="15" height="15" />
            <path transform="scale(0.23) translate(20,18)" fill="#FFFFFF" d="M22.245,4.015c0.313,0.313,0.313,0.826,0,1.139l-6.276,6.27c-0.313,0.312-0.313,0.826,0,1.14l6.273,6.272  c0.313,0.313,0.313,0.826,0,1.14l-2.285,2.277c-0.314,0.312-0.828,0.312-1.142,0l-6.271-6.271c-0.313-0.313-0.828-0.313-1.141,0  l-6.276,6.267c-0.313,0.313-0.828,0.313-1.141,0l-2.282-2.28c-0.313-0.313-0.313-0.826,0-1.14l6.278-6.269  c0.313-0.312,0.313-0.826,0-1.14L1.709,5.147c-0.314-0.313-0.314-0.827,0-1.14l2.284-2.278C4.308,1.417,4.821,1.417,5.135,1.73  L11.405,8c0.314,0.314,0.828,0.314,1.141,0.001l6.276-6.267c0.312-0.312,0.826-0.312,1.141,0L22.245,4.015z"
            />
        </g>

        <!-- Chord -->
        <text x="50" y="10" class="chord">
            <%= chord %>
        </text>

        <!-- Frets -->
        <% _.each(frets, function(fret, index){ %>
            <line x1="<%= fret.x %>" y1="15" x2="<%= fret.x %>" y2="42" class="fret"></line>
            <text x="<%= fret.x + 5 %>" y="42" class="fret-number">
                <%= fret.number %>
            </text>
            <% }); %>

                <!-- Strings -->
                <% _.each(strings, function(string, index){ %>
                    <line x1="12" y1="<%= string.y %>" x2="88" y2="<%= string.y %>" class="string <%= string.state %>"></line>
                    <% }); %>

                        <!-- Strings Played -->
                        <% _.each(fingers, function(finger, index){ %>
                            <line x1="12" y1="<%= finger.y %>" x2="<%= finger.x %>" y2="<%= finger.y %>" class="string fingered"></line>
                            <% }); %>

                                <!-- Fingers -->
                                <% _.each(fingers, function(finger, index){ %>
                                    <circle cx="<%= finger.x %>" cy="<%= finger.y %>" r="1.7" class="finger" />
                                    <% }); %>

    </svg>
</div>