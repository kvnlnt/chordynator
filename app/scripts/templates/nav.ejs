<div class="primers">
    <ul class="items">
        <li class="item">Theory Primers</li>
    </ul>
</div>

<div class="words">
    <ul class="items">
        <li class="item find"><input type="text" placeholder="WORD" /></li>
        <li class="item add"><a>+</a></li>
    </ul>
</div>

<div class="plats">
    <ul class="items keys bases">
        <% for(var key in keys) { %>
        <li><a class="item key" data-key="<%= keys[key] %>"><%= keys[key] %></a></li>
        <% } %>
        <li><a class="item next">+</a></li>
    </ul>
    <ul class="items keys flats">
        <% for(var key in keys) { %>
        <li><a class="item key" data-key="<%= keys[key] %>b"><%= keys[key] %><sup>♭</sup></a></li>
        <% } %>
        <li><a class="item next">+</a></li>
    </ul>
    <ul class="items keys sharps">
        <% for(var key in keys) { %>
        <li><a class="item key" data-key="<%= keys[key] %>#"><%= keys[key] %><sup>♯</sup></a></li>
        <% } %>
        <li><a class="item next">+</a></li>
    </ul>
</div>

<div class="tabs">
    <ul class="items">
        <li class="item find"><input type="text" placeholder="CHORD" /></li>
        <li class="item add"><a>+</a></li>
    </ul>
</div>
