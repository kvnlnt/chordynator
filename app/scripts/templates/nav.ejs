<div class="menu">
    <ul class="items">
        <li><a class="item nav_home showing" item="home"><i class="fa fa-home"></i></a></li>
        <li><a class="item nav_plats"  item="plats"><i class="fa fa-sitemap"></i></a></li>
        <li><a class="item nav_tabs"   item="tabs"><i class="fa fa-search"></i></a></li>
        <li><a class="item nav_words"  item="words"><i class="fa fa-pencil"></i></a></li>
